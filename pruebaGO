package main

import "fmt"

func main() {
var valor1, valor2 int = 4, 2
fmt.Println("Los valores son:", valor1, "y", valor2)

var operador string = "x"
fmt.Println("El operador es:", operador)

var resultado int

switch operador {
  case "+":
    resultado = valor1 + valor2
  case "-":
    resultado = valor1 - valor2
  case "x":
    resultado = valor1 * valor2
  case "/":
    resultado = valor1 / valor2
}

fmt.Println("El resultado es:", resultado)

}
